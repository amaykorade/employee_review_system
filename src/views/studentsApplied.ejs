<h1>Interviews</h1>
<table class="table">
  <thead>
    <tr>
      <% if(data && data.length > 0){ %>
      <th scope="col">Students Applied for <%= data[0].company_name %></th>
      <% } %>
    </tr>
  </thead>
  <tbody>
    <% if(message){ %>
    <div class="alert alert-dark" role="alert"><%= message %></div>
    <% } %> <% data.map(students => { %>
    <tr>
      <!-- <th scope="row">1</th> -->
      <td><%= students.name %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
