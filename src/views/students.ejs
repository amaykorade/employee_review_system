<h1>Students</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">name</th>
      <th scope="col">Interviews</th>
      <th scope="col">Results</th>
      <th scope="col">Interview_Date</th>
      <th scope="col">View</th>
    </tr>
  </thead>
  <tbody>
    <% data.map(data => { %>
    <tr>
      <th scope="row">1</th>
      <td><%= data.name %></td>
      <td><%= data.company_name%></td>
      <td><%= data.result%></td>
      <td>
        <%= new Date(data.interview_date).toLocaleDateString('en-US', { weekday:
        'long', year: 'numeric', month: 'long', day: 'numeric' })%>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
